   ],
   "source": [
    "def fibonacci(n):\n",
    "    if n==1 or n==2:\n",
    "        return 1\n",
    "    return fibonacci(n-1) + fibonacci(n-2)\n",
    "for i in range (1, 11):\n",
    "    print (fibonacci(i))"
   ]
