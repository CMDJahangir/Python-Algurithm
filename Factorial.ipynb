   "source": [
    "def factorial_number(xyz):\n",
    "    if xyz == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return xyz * factorial_number(xyz-1)\n",
    "xyz=int(input(\"Input Your Number : \"))\n",
    "print(factorial_number(xyz))"
   ]
